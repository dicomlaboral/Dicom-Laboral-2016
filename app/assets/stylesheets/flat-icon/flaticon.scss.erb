@font-face {
	font-family: "Flaticon";
	src: url("<%= asset_path('flat-icon/flaticon.eot')%>");
	src: url("<%= asset_path('flat-icon/flaticon.eot#iefix')%>") format("embedded-opentype"),
	url("<%= asset_path('flat-icon/flaticon.woff')%>") format("woff"),
	url("<%= asset_path('flat-icon/flaticon.ttf')%>") format("truetype"),
	url("<%= asset_path('flat-icon/flaticon.svg')%>") format("svg");
	font-weight: normal;
	font-style: normal;
}
